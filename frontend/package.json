{
  "name": "gameplan-ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/assets/gameplan/frontend/ && yarn copy-html-entry",
    "copy-html-entry": "cp ../gameplan/public/frontend/index.html ../gameplan/www/g.html",
    "serve": "vite preview",
    "test-local": "cypress open --e2e --browser chrome",
    "test": "cypress run --record --key 9c23c39a-b56b-46d5-b673-44faf1f1da83",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@sentry/vue": "^8.37.1",
    "@vueuse/core": "^10.1.2",
    "dayjs": "^1.10.7",
    "feather-icons": "^4.28.0",
    "frappe-ui": "^0.1.87",
    "fuzzysort": "^2.0.4",
    "gemoji": "^7.1.0",
    "htmldiff-js": "^1.0.5",
    "socket.io-client": "^4.7.2",
    "vue": "3.4.12",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.2",
    "cypress": "10.11.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "lucide-static": "^0.257.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "rollup-plugin-visualizer": "^5.8.2",
    "tailwindcss": "^3.4.15",
    "unplugin-icons": "^0.15.1",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^6.0.3",
    "vite-plugin-pwa": "^0.21.1",
    "vue-template-compiler": "^2.7.16"
  }
